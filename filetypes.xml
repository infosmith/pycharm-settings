<application>
  <component name="FileTypeManager" version="18">
    <ignoreFiles list="*$py.class;*.pyc;*.pyo;*.rbc;*.yarb;*~;.DS_Store;.git;.hg;.svn;CVS;__pycache__;_svn;vssver.scc;vssver2.scc;" />
    <extensionMap>
      <mapping pattern=".env" type="Env File" />
      <mapping pattern=".envrc.template" type="PLAIN_TEXT" />
      <mapping pattern=".gitattributes" type="PLAIN_TEXT" />
      <mapping pattern=".env.example" type="PLAIN_TEXT" />
      <mapping pattern=".envrc" type="PLAIN_TEXT" />
      <mapping pattern=".python-version" type="PLAIN_TEXT" />
      <mapping pattern=".gitkeep" type="PLAIN_TEXT" />
      <mapping ext="in" type="PLAIN_TEXT" />
      <mapping ext="opml" type="XML" />
      <removed_mapping pattern=".env" approved="true" type=".env file" />
      <mapping pattern=".gitignore_global" type="Git file" />
    </extensionMap>
  </component>
</application>